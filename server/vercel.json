{
    "version": 2,
    "routes": [
      {
        "src": "/socket.io/(.*)",
        "dest": "https://xoxreact-servers.vercel.app/socket.io/$1",
        "headers": {
          "Connection": "upgrade",
          "Upgrade": "websocket"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/index.js"
      }
    ]
  }
  